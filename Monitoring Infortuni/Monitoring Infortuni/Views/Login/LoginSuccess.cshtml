@model IEnumerable<Monitoring_Infortuni.Models.SediModel>

<p>
    @Html.ActionLink("Crea Una Nuova Sede", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Sede)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Sede)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Data)
        </td>
        <td>
            @Html.ActionLink("Modifica", "Edit", new { id=item.Id })
            @Html.ActionLink("Elimina", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
